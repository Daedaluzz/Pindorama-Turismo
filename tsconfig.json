{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/get-dictionary.ts",
    "src/middleware.ts",
    "src/middleware.ts",
    "src/i18n",
    "src/get-dictionarys"
, "src/app/[locale]/components/nav/nav.tsx", "src/app/[locale]/components/localeSwitch/localeSwitcher.tsx", "src/app/[locale]/components/localeSwitch/localeSwitchSelect.tsx", "src/components/nav/nav2"  ],
  "exclude": [
    "node_modules"
  ]
}
